{
  "name": "bananacss",
  "description": "The Brazilian css superset",
  "version": "0.7.0",
  "author": {
    "name": "afonsopacifer",
    "email": "afonsopacifer@live.com"
  },
  "bugs": {
    "url": "https://github.com/bananacss/bananacss/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bananacss/bananacss"
  },
  "keywords": [
    "css",
    "parser",
    "style",
    "preprocessor",
    "stylesheets",
    "superset",
    "language"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "main": "dist/banana.js",
  "bin": {
    "banana": "dist/bin/index.js"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "chokidar": "^1.6.0",
    "commander": "^2.9.0",
    "css": "^2.2.1",
    "lodash": "^4.15.0"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build",
    "cover": "npm run build && babel-istanbul cover node_modules/mocha/bin/_mocha -- ./test/index.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "test": "npm run build && mocha test/index --compilers babel-core/register",
    "test-properties": "npm run build && mocha test/properties.test --compilers babel-core/register",
    "eslint": "eslint src/* src/core/* src/helpers/* src/bin/* test/*",
    "jsdocs": "jsdoc src/bin/*.js src/*.js src/core/*.js src/helpers/*.js"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-istanbul": "^0.11.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.2.2",
    "jsdoc": "^3.4.0",
    "mocha": "^3.0.0",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
